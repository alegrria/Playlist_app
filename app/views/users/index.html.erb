<%= flash[:notice] %>
<table>
  <thead>
  	<tr>
  	  <th>First Name</th>
  	  <th>Last Name</th>
  	  <th>Email</th>
  	  <th>Username</th>
  	</tr>
  </thead>
  <tbody>
  	<% @users.each do |user| %>
  	  <tr>
  	  	<td><%= user.first_name %></td>
  	  	<td><%= user.last_name %></td>
  	  	<td><%= user.email %></td>
  	  	<td><%= user.user_name %></td>
  	  </tr>
  	<% end %>
  </tbody>
</table>
<div>

<h4>Import data</h4>
  <%= form_tag import_users_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>